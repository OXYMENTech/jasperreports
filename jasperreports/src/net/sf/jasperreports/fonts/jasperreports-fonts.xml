<?xml version="1.0" encoding="UTF-8"?>

<fontFamilies>

	<fontFamily name="JasperReports Icons" visible="false">
		<normal>
			<ttf>net/sf/jasperreports/fonts/icons/icons.ttf</ttf>
			<svg>net/sf/jasperreports/fonts/icons/icons.svg</svg>
			<eot>net/sf/jasperreports/fonts/icons/icons.eot</eot>
			<woff>net/sf/jasperreports/fonts/icons/icons.woff</woff>
		</normal>
		<pdfEncoding>Identity-H</pdfEncoding>
		<pdfEmbedded>true</pdfEmbedded>
	</fontFamily>

	<!-- 
		Deprecated and no longer used. Kept for backward compatibility with persisted
		generated reports in JasperPrint or JRPXML format.
	-->
	<fontFamily name="Pictonic" visible="false">
		<normal>
			<ttf>net/sf/jasperreports/fonts/icons/icons.ttf</ttf>
			<svg>net/sf/jasperreports/fonts/icons/icons.svg</svg>
			<eot>net/sf/jasperreports/fonts/icons/icons.eot</eot>
			<woff>net/sf/jasperreports/fonts/icons/icons.woff</woff>
		</normal>
		<pdfEncoding>Identity-H</pdfEncoding>
		<pdfEmbedded>true</pdfEmbedded>
	</fontFamily>


	<fontFamily name="Source Han Sans SC">
        <normal>net/sf/jasperreports/fonts/SourceHanSansSC-Normal.ttf</normal>
        <bold>net/sf/jasperreports/fonts/SourceHanSansSC-Bold.ttf</bold>
        <italic>net/sf/jasperreports/fonts/SourceHanSansSC-Regular.ttf</italic>
        <pdfEncoding>Identity-H</pdfEncoding>
        <pdfEmbedded>true</pdfEmbedded>
        <exportFonts>
            <export key="net.sf.jasperreports.html">'Source Han Sans SC'</export>
            <export key="net.sf.jasperreports.xhtml">'Source Han Sans SC'</export>
        </exportFonts>
    </fontFamily>

    <fontFamily name="Lato">
        <normal>net/sf/jasperreports/fonts/SourceHanSansSC-Normal.ttf</normal>
        <bold>net/sf/jasperreports/fonts/SourceHanSansSC-Bold.ttf</bold>
        <italic>net/sf/jasperreports/fonts/SourceHanSansSC-Regular.ttf</italic>
        <pdfEncoding>Identity-H</pdfEncoding>
        <pdfEmbedded>true</pdfEmbedded>
        <exportFonts>
            <export key="net.sf.jasperreports.html">Lato</export>
            <export key="net.sf.jasperreports.xhtml">Lato</export>
        </exportFonts>
    </fontFamily>

</fontFamilies>
